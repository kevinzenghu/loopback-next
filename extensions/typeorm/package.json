{
  "name": "@loopback/typeorm",
  "version": "0.0.1",
  "description": "LoopBack TypeORM Integration",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-typeorm*.tgz dist tsconfig.build.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-typeorm*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/core": "^2.4.1",
    "tslib": "^1.11.1",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@loopback/build": "^5.2.0",
    "@loopback/eslint-config": "^6.0.4",
    "@loopback/testlab": "^3.1.1",
    "@types/node": "^10.17.21",
    "sqlite3": "^4.1.1"
  },
  "keywords": [
    "LoopBack",
    "TypeORM",
    "Database"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "extensions/typeorm"
  }
}
